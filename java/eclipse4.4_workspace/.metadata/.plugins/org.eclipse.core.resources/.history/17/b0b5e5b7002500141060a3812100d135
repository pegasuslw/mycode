import java.util.Arrays;
import java.util.List;


public class Client {

	public static void main(String[] args){
		int[] a = {1,2,3,4,5};
    	List list = Arrays.asList(a);
		System.out.println("int数组的大小:" + list.size());
		
		int num = 5;
		Integer[] b = new Integer[num];
		for(int i =0;i<num;i++){
			 b[i] = i;
		}
		List list2 = Arrays.asList(b);
		System.out.println("Integer数组的大小:" + list2.size());
		
	}
}
