import java.util.Arrays;
import java.util.List;


public class Client {

	public static void main(String[] args){
		int[] a = {1,2,3,4,5};
    	List list = Arrays.asList(a);
		System.out.println(list.size());
		
		int num = 5;
		Integer[] b = new Integer[num];
		for(Integer i : b)
		{
			b[i] = i;
			
		}
		
	}
}
